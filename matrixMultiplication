#include<stdio.h>
#include<conio.h>
int main()
{
    int p,q,r,c,i,j,k,a[10][10],b[10][10],m[10][10];
    printf("enter row:");
    scanf("%d",&r);
    printf("enter collumn:");
    scanf("%d",&c);
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            printf("%d\t",a[i][j]);
        }
        printf("\n");
    }
    
    printf("enter row for b[][]:");
    scanf("%d",&r);
    printf("enter collumn:");
    scanf("%d",&c);
    for(p=0;p<r;p++)
    {
        for(q=0;q<c;q++)
        {
            scanf("%d",&b[p][q]);
        }
    }
    
    for(p=0;p<r;p++)
    {
        for(q=0;q<c;q++)
        {
            printf("%d\t",b[p][q]);
        }
        printf("\n");
    }
    
    if(p!=j)
    {
        printf("multilication not mpossible!");
    }
    else
    {
     for(i=0;i<r;i++)
    {
        for(p=0;p<q;p++)
        {
            m[i][p]=0;
            for(k=0;k<p;k++)
            {
                m[i][k]=a[i][k]*b[k][j];
            }
        }
    }
    }
    
    for(i=0;i<r;i++)
    {
        for(p=0;p<q;p++)
        {
        printf("%d\t",m[i][k]);
        }
        printf("\n");
    }
    
    return 0;
}
